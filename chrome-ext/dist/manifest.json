{
  "manifest_version": 3,
  "name": "Agapi2",
  "description": "Mental Health Assistant",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "./cheems.png"
  },"background": {
    "service_worker": "./scripts/background.js" 
  },
  "permissions": [
    "scripting", "activeTab"
  ],
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["./scripts/contentScript.js"]
    }
]
}